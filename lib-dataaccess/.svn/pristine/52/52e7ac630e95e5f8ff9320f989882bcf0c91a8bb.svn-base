package com.goldwind.dataaccess.rtdb.model;

import java.util.Date;

/**
 * 实时数据实体类
 * 
 * @author shenlf
 *
 */
public class DataEntity
{
    /**
     * 测点id
     */
    private int id;
    /**
     * 测点名称
     */
    private String tagName;
    /**
     * 测点时间
     */
    private Date time;
    /**
     * 测点值
     */
    private Object value;
    /**
     * 数据类型
     */
    private RTDataType type;

    public int getId()
    {
        return id;
    }

    public void setId(int id)
    {
        this.id = id;
    }

    public String getTagName()
    {
        return tagName;
    }

    public void setTagName(String tagName)
    {
        this.tagName = tagName;
    }

    public Date getTime()
    {
        return time;
    }

    public void setTime(Date time)
    {
        this.time = time;
    }

    public Object getValue()
    {
        return value;
    }

    public void setValue(Object value)
    {
        this.value = value;
    }

    public RTDataType getType()
    {
        return type;
    }

    public void setType(RTDataType type)
    {
        this.type = type;
    }

    @Override
    public String toString()
    {
        return "tagName:" + tagName + "  value:" + value + "  time:" + time;
    }

}
