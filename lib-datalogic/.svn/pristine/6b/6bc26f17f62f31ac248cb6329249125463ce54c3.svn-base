package com.goldwind.datalogic.business;

import com.goldwind.datalogic.business.BusinessDef.ExpressType;

/**
 * 公式计算结果类
 * 
 * @author 冯春源
 *
 */
public class FormulaResult
{
    /**
     * 计算类型，1-四则运算(取四则运算结果)，2-关系运算(取关系/逻辑运算结果)，3-逻辑运算(取关系/逻辑运算结果)
     */
    private ExpressType expressType;

    /**
     * 四则运算结果
     */
    private double arithmeticResult;

    /**
     * 关系/逻辑运算结果
     */
    protected boolean logicResult;

    public ExpressType getExpressType()
    {
        return expressType;
    }

    public void setExpressType(ExpressType expressType)
    {
        this.expressType = expressType;
    }

    public double getArithmeticResult()
    {
        return arithmeticResult;
    }

    public void setArithmeticResult(double arithmeticResult)
    {
        this.arithmeticResult = arithmeticResult;
    }

    public boolean getLogicResult()
    {
        return logicResult;
    }

    public void setLogicResult(boolean logicResult)
    {
        this.logicResult = logicResult;
    }

}
