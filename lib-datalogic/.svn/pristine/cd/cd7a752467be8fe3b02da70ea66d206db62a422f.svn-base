package com.goldwind.datalogic.business.model;

/**
 * 故障状态信息
 * 
 * @author 冯春源
 *
 */
public class FaultStateInfo
{
    /**
     * 机型
     */
    private String protype;

    /**
     * 主故障状态
     */
    private String faultstate;

    /**
     * 主故障标志（1-主故障变位数据维护total，0-以故障串首位维护total）
     */
    private String mainfault;

    public FaultStateInfo(String protype, String faultstate, String mainfault)
    {
        this.protype = protype;
        this.faultstate = faultstate;
        this.mainfault = mainfault;
    }

    public String getProtype()
    {
        return protype;
    }

    public String getFaultState()
    {
        return faultstate;
    }

    public String getMainFault()
    {
        return mainfault;
    }
}
